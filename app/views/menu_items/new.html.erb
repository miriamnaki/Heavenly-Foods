<%= form_with model:@menu_item, local: true do |f|%>
<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.label :price %>
<%= f.number_field :price %>

<%= f.label :image%>
<%= f.file_field :image%>



<%= f.submit 'Submit' %>
<% end %>